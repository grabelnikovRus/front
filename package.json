{
  "name": "ru-site-front",
  "version": "5.2.0",
  "private": true,
  "description": "pik-broker.ru web frontend",
  "scripts": {
    "api:fetch": "cp ../new-mainsite-back/http/swagger.yaml schema.yaml",
    "preapi:generate": "rimraf src/api",
    "api:generate": "openapi-generator generate -i schema.yaml -g typescript-fetch -o src/api -c config/api/config.json",
    "postapi:generate": "prettier src/api -w",
    "api:template": "openapi-generator author template -g typescript-fetch -o config/api/template",
    "api:validate": "openapi-generator validate -i schema.yaml",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c config/storybook",
    "storybook:build": "build-storybook -c config/storybook",
    "preextract:svg": "rimraf src/uikit/svg",
    "extract:svg": "npx @svgr/cli --config-file config/svgr/config.js --out-dir src/uikit/svg src/assets/svg/",
    "update:deps": "yarn upgrade-interactive --latest",
    "update:cssdb": "npx browserslist@latest --update-db",
    "test": "jest",
    "hygen": "HYGEN_TMPLS=config/hygen npx hygen",
    "lint:eslint": "eslint . --report-unused-disable-directives",
    "lint:stylelint": "stylelint 'src/**/*.?(s)css' --report-needless-disables",
    "lint:types": "tsc --noEmit",
    "lint:ls": "ls-lint",
    "lint:editorconfig": "editorconfig-checker -config config/editorconfig-checker.json",
    "lint:tests": "jest",
    "lint-all": "concurrently yarn:lint:*"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@material-ui/core": "^4.11.4",
    "@n8tb1t/use-scroll-position": "^2.0.3",
