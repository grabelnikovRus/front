    "@react-pdf-viewer/locale-switcher": "3.1.1",
    "@react-pdf-viewer/thumbnail": "3.1.1",
    "@react-pdf-viewer/toolbar": "3.1.1",
    "@sentry/nextjs": "^6.18.1",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "detect-browser": "^5.3.0",
    "downshift": "^6.1.7",
    "final-form": "^4.20.6",
    "final-form-arrays": "^3.0.2",
    "final-form-focus": "^1.1.2",
